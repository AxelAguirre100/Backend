<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="/css/style.css">
</head>

<body>
    <div>
        <form id="registerFormId" class="loginForm">
            <div class="form-outline mb-4">
                <label class="form-label" for="form2Example2">First Name</label>
                <input type="text" id="first_name" class="form-control inputLogin" required/>
            </div>
            <div class="form-outline mb-4">
                <label class="form-label" for="form2Example2">Last Name</label>
                <input type="text" id="last_name" class="form-control inputLogin" required/>
            </div>
            <div class="form-outline mb-4">
                <label class="form-label" for="form2Example2">age</label>
                <input type="number" id="birth_date" class="form-control inputLogin" required />
            </div>
            <div class="form-outline mb-4">
                <label class="form-label" for="form2Example1">Email address</label>
                <input type="email" id="email" class="form-control inputLogin" required/>
            </div>
            <div class="form-outline mb-4">
                <label class="form-label" for="form2Example2">Password</label>
                <input type="password" id="password" class="form-control inputLogin" required/>
            </div>
            <button id="btnLogin" type="submit" class="btn btn-primary btn-block mb-4">Sign Up</button>
            <div class="text-center">
                <p>Already registered? <a id="hrefLogin">Login</a></p>
            </div>
        </form>
    </div>
    <script type="module" src="/js/register.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.0.19/dist/sweetalert2.min.js"></script>

</body>

</html>